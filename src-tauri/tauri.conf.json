{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "backstage68",
  "version": "0.1.0",
  "identifier": "com.backstage68.app",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [],
    "security": {
      "csp": "default-src 'self' 'unsafe-inline' 'unsafe-eval' http: https: data: blob: tauri: *",
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": null,
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    },
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
